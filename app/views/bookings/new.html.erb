<h1>New Booking</h1>

<%= render 'form' %>

<p id="notice"><%= notice %></p>


<table>
  <thead>
  <tr>
    <th>Category</th>
    <th>Model</th>
    <th>Manufacturer</th>
    <th>Is AC</th>
    <th>Cost</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @vehicles.each do |vehicle| %>
    <tr>
      <td><%= vehicle.category %></td>
      <td><%= vehicle.model %></td>
      <td><%= vehicle.manufacturer %></td>
      <td><%= vehicle.isAc %></td>
      <td><%= vehicle.cost %></td>

    </tr>
  <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Back', bookings_path %>
